{
  "name": "react-jam-2023-winter",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "typecheck": "tsc --noEmit",
    "build": "npm run lint && tsc && vite build",
    "update-info": "npx rune-games-cli@latest update-info",
    "upload:draft": "npx rune-games-cli@latest upload ./dist --draft --name Switchback",
    "upload": "npx rune-games-cli@latest upload ./dist --release --name Switchback",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives",
    "preview": "vite preview"
  },
  "dependencies": {
    "@fontsource/inter": "^5.0.16",
    "@react-three/drei": "^9.92.7",
    "@react-three/fiber": "^8.15.12",
    "@react-three/postprocessing": "^2.15.11",
    "clsx": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rune-games-sdk": "^4.16.17",
    "three": "^0.160.0",
    "tone": "^14.7.77",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/three": "^0.160.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "7.22.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-rune": "^0.2.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-plugin-qrcode": "^0.2.2",
    "vite-plugin-rune": "^0.2.0"
  }
}
